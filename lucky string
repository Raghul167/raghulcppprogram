#include<iostream>
#include<string.h>
using namespace std;
int main()
{
   int a[100];
   int sum=0;
   string str;
   getline(cin,str);
   for(int i=0;i<str.length();i++)
   {
     a[i]=str[i];
     sum=sum+a[i];
   }

   if(sum%2==0)
   {
       cout<<str<<" is lucky";
   }
   else
   {
       cout<<str<<" is not lucky";
   }
}

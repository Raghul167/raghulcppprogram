#include<iostream>
using namespace std;
int main()
{
    char a;
    cin>>a;
    int b=(int)a;
    if(b>=97&&b<=122)
    {
        cout<<"Lower";
    }
    else if(b>=65&&b<=90)
    {
        cout<<"Upper";
    }
    else if(b>=48&&b<=57)
    {
        cout<<"Number";
    }
    else
    {
        cout<<"Symbol";
    }
    return 0;
}

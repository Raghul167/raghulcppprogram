#include<iostream>
using namespace std;
int main()
{
    int m,h,s;
    cin>>h>>m>>s;
    if(s>=60)
    {
        m=m+1;
        s=s-60;
    }
    else if(m>=60)
    {
        h=h+1;
        m=m-60;
    }
    cout<<"Total Number of hours is "<<h<<endl;
    cout<<"Total Number of minutes is "<<m<<endl;
    cout<<"Total Number of seconds is "<<s<<endl;
}

#include<iostream>
using namespace std;
int main()
{
    int n,sum1=0,sum2=0,i;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    
    if(a[i]%2==0)
    {
        sum1++;
    }
    
    else
    {
        sum2++;
    }
    
    }
    cout<<"Odd:"<<sum2<<endl;
    cout<<"Even:"<<sum1;
}

#include<iostream>
using namespace std;
int main()
{
    int n1,i,j;
    cin>>n1;
    int a[n1];int x[n1];
     for(i=0;i<n1;i++)
    {
        cin>>a[i];
        
    }
    int b[n1];
    for(i=0;i<n1;i++)
    {
        cin>>b[i];
         
       
    }
     for(i=0;i<n1;i++)
    {
        x[i]=a[i]+b[i];
        cout<<x[i]<<" ";
    }
   
    //int a[i]=[a[i] + b[j]];
    // cout<<a[i];
}
